<script>
  new Morris.Bar({
    element: 'chart',
    data: [
      <% @reward_popularity.each do |key, value| %>
        { amount: '<%= human_currency key.to_i %>', popularity: <%= value.to_i %> },
      <% end  %>
    ],
    xkey: 'amount',
    ykeys: ['popularity'],
    labels: ['Popularity']
  });  
</script>